  "minimum_entity_count": 6,
  "validation_rules": {
    "must_detect": ["Customer", "Product", "Cart", "Order", "OrderItem"],
    "should_detect": ["CartItem", "ProductVariant", "Category"],
    "may_detect": ["ShippingAddress", "PaymentMethod", "Inventory"]
