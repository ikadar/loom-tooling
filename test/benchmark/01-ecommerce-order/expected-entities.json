    {
      "name": "ShippingAddress",
      "type": "entity_or_value_object",
      "confidence": "medium",
      "attributes_expected": ["street", "city", "postalCode", "country"],
      "relationships_expected": ["Order"],
      "notes": "Mentioned but unclear if entity or value object"
    }
